{% extends 'BackendAdminBundle::layout.html.twig' %}

{% block content %}
<div id="content">
    <h1>Vols creation</h1>

    <form action="{{ path('vols_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <p>
            <button type="submit">Create</button>
        </p>
    </form>

    <ul class="record_actions">
        <li>
            <a href="{{ path('vols') }}">
                Back to the list
            </a>
        </li>
    </ul>
</div>
{% endblock %}