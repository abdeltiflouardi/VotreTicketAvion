{% extends 'BackendAdminBundle::layout.html.twig' %}

{% block content %}
<div id="content">
    <h1>Profile</h1>
    <form action="{{ path('BackendAdminBundle_profile_index') }}" method="post" {{ form_enctype(form) }}>
        <fieldset>
            <legend>Modifier votre login</legend>
            {{ form_widget(form) }}

            <input type="submit" value="{{ "Save"|trans }}" />
        </fieldset>
    </form>
</div>
{% endblock %}